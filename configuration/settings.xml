<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          http://maven.apache.org/xsd/settings-1.0.0.xsd">
      <profiles>
        <profile>
				<id>build.repos</id>
                <repositories>
					<repository>
						<id>central</id>
						<name>Maven Central</name>
						<url>http://nexus-test.192.168.99.100.nip.io/nexus/content/repositories/central/</url>
						<snapshots>
							<enabled>false</enabled>
						</snapshots>
						<releases>
							<enabled>true</enabled>
							<updatePolicy>never</updatePolicy>
						</releases>
					</repository>
				</repositories>
				<pluginRepositories>
					<pluginRepository>
						<id>central</id>
						<name>Maven Central</name>
						<url>http://nexus/nexus/content/repositories/central/</url>
						<snapshots>
							<enabled>false</enabled>
						</snapshots>
						<releases>
							<enabled>true</enabled>
							<updatePolicy>never</updatePolicy>
						</releases>
					</pluginRepository>
				</pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>build.repos</activeProfile>
    </activeProfiles>
</settings>
